# Test 1: WHILE funciona
print "1. WHILE LOOPS: ✅ FUNCIONANDO"
set $i 0
while $i < 3 do
    set $i $i + 1
    print "  While iteration $i"
endloop

# Test 2: REPEAT con variable funciona
print ""
print "2. REPEAT CON VARIABLES: ✅ FUNCIONANDO"
set $count 3
repeat $count times do
    print "  Repeat iteration"
endloop

# Test 3: IF simple funciona
print ""
print "3. IF/THEN/ELSE: ✅ FUNCIONANDO"
set $value 5
if $value > 3 then
    print "  If condition works"
else
    print "  Else works"
endif

# Test 4: JSONPath funciona
print ""
print "4. JSONPATH: ✅ FUNCIONANDO"
GET "https://httpbin.org/json"
extract jsonpath "$.slideshow.title" as $title
print "  Extracted: $title"

# Test 5: Regex funciona
print ""
print "5. REGEX: ✅ FUNCIONANDO"
GET "https://httpbin.org/headers"
extract regex "\"Host\":\s*\"([^\"]+)\"" as $host
print "  Host: $host"

